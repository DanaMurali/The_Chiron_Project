{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "module": "ESNext",
    "isolatedModules": true,
    "jsx": "preserve",
    "allowJs": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
